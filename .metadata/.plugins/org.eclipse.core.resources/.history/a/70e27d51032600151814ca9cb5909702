import java.util.List;
import java.util.Map;

/**
 * 
 */

/**
 * @author kumaksha
 *
 */
public class TreeNode {
	
	private String value;
	private List<Map<String, String>> tupple;
	private TreeNode leftChild, rightChild;

	/**
	 * 
	 */
	public TreeNode(String value, List<Map<String, String>> tupple, TreeNode leftChild, TreeNode rightChild) {
		this.value = value;
		this.leftChild = leftChild;
		this.rightChild = rightChild;
		this.tupple = tupple;
	}

}
