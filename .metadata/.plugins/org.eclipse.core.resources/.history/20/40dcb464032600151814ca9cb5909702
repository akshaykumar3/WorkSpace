import java.util.List;
import java.util.Map;

/**
 * 
 */

/**
 * @author kumaksha
 *
 */
public class TreeNode {
	
	private String value;
	private List<Map<String, String>> tupple;
	private TreeNode leftChild, rightChild;

	/**
	 * 
	 */
	public TreeNode(String value, List<Map<String, String>> tupple, TreeNode leftChild, TreeNode rightChild) {
		this.value = value;
		this.leftChild = leftChild;
		this.rightChild = rightChild;
		this.tupple = tupple;
	}

	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}

	public List<Map<String, String>> getTupple() {
		return tupple;
	}

	public void setTupple(List<Map<String, String>> tupple) {
		this.tupple = tupple;
	}

	public TreeNode getLeftChild() {
		return leftChild;
	}

	public void setLeftChild(TreeNode leftChild) {
		this.leftChild = leftChild;
	}

	public TreeNode getRightChild() {
		return rightChild;
	}

	public void setRightChild(TreeNode rightChild) {
		this.rightChild = rightChild;
	}
}
